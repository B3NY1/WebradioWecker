                  <Button fx:id="but_stop" mnemonicParsing="false" onAction="#butstop" prefHeight="50.0" prefWidth="50.0" text="Stop" GridPane.rowIndex="4" />
               </children>
            </GridPane>
            <VBox prefHeight="200.0" prefWidth="100.0" spacing="5.0">
               <children>
                  <Button mnemonicParsing="false" prefHeight="50.0" prefWidth="100.0" text="Radio" />
                  <Button mnemonicParsing="false" prefHeight="50.0" prefWidth="100.0" text="Wecker" />
               </children>
            </VBox>
